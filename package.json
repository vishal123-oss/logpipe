{
  "name": "log-pipe",
  "version": "1.0.0",
  "description": "Custom Message Broker - LogPipe",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "example:producer": "ts-node examples/test-producer.ts",
    "example:consumer": "ts-node examples/test-consumer.ts",
    "example:test": "ts-node examples/test-logpipe.ts",
    "example:offsets": "ts-node examples/test-offset-features.ts",
    "example:multi": "ts-node examples/test-multi-topic-consumer.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "message-broker",
    "log-pipe",
    "event-log",
    "nodejs"
  ],
  "author": "AI Coding Agent",
